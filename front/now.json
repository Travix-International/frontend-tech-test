{
  "version": 2,
  "name": "hellofresh-test",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    { "src": "^/favicon.ico", "dest": "/favicon.ico" },
    {
      "src": "^/main.js",
      "headers": { "cache-control": "s-maxage=0" },
      "dest": "/main.js"
    },
    {
      "src": "^/(.*)",
      "headers": { "cache-control": "s-maxage=0" },
      "dest": "/index.html"
    }
  ]
}
