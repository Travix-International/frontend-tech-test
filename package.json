{
  "name": "test-candidates",
  "version": "0.0.1",
  "description": "test back - front",
  "scripts": {
    "build-css": "node-less-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-less-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "server":"node ./src/server/server.js",
    "start": "npm-run-all -p watch-css start-js server",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "keywords": [
    "test",
    "travix"
  ],
  "author": "spartans",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "node-less-chokidar": "^0.1.2",
    "npm-run-all": "^4.1.3",
    "react": "^16.3.2",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.3.2",
    "react-fontawesome": "^1.6.1",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.4",
    "react-toastify": "^4.0.1",
    "react-websocket": "^2.0.0",
    "redux": "^4.0.0",
    "redux-mock-store": "^1.5.1",
    "redux-saga": "^0.16.0",
    "supertest": "^3.1.0",
    "ws": "^5.2.0"
  },
  "devDependencies": {
    "jest": "^22.4.4",
    "mock-socket": "^7.1.0"
  }
}
