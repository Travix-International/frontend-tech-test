{
  "name": "test-candidates",
  "version": "0.0.1",
  "description": "test back - front",
  "jest": {
    "moduleNameMapper": {
      "\\.(scss|css|jpg|png|svg)$": "<rootDir>/empty-module.js"
    },
    "automock": false
  },
  "eslintConfig": {
    "extends": "travix"
  },
  "scripts": {
    "test": "jest client",
    "start": "concurrently --kill-others-on-fail 'yarn server' 'yarn client'",
    "server": "nodemon server",
    "client": "cd client && yarn start",
    "dev": "run-p server start",
    "test:api": "mocha --exit --opts ./mocha.opts",
    "lint": "eslint . --ext .js --ext .jsx --cache",
    "lint:fix": "eslint . --ext .js --ext .jsx --cache --fix"
  },
  "keywords": [
    "test"
  ],
  "author": "spartans",
  "license": "ISC",
  "dependencies": {
    "jest": "^24.1.0",
    "node-sass": "^4.11.0",
    "react-scripts": "3.0.0",
    "superagent": "^4.1.0",
    "uniqid": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "concurrently": "^4.0.1",
    "cors": "^2.8.5",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-travix": "5.8.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-hooks": "^1.3.0",
    "express": "^4.17.1",
    "mocha": "^6.1.4",
    "node-env-run": "^3.0.2",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "supertest": "^4.0.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
