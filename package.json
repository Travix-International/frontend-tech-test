{
  "name": "test-candidates",
  "version": "0.0.1",
  "description": "test back - front",
  "main": "server/server.js",
  "scripts": {
    "install:all": "npm-run-all -p install:server install:client",
    "install:client": "npm install --prefix ./client",
    "install:server": "npm install",
    "test:server": "./node_modules/.bin/jest server --watch",
    "test:client": "npm test --prefix ./client",
    "test:unit": "npm-run-all -p test:server test:client",
    "test:integration": "./node_modules/.bin/jest integration",
    "start:server": "node server/server.js",
    "start:client": "npm start --prefix ./client",
    "start": "npm-run-all -p start:server start:client"
  },
  "keywords": [
    "test",
    "travix"
  ],
  "author": "spartans",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.4",
    "express": "^4.14.0"
  },
  "devDependencies": {
    "jest": "^21.2.1",
    "nightmare": "^2.10.0",
    "npm-run-all": "^4.1.1",
    "superagent": "^3.6.1",
    "supertest": "^3.0.0"
  }
}
