{
  "name": "test-candidates",
  "version": "0.0.1",
  "description": "test back - front",
  "main": "server.js",
  "scripts": {
    "test": "mocha \"pages/**/*.test.js\" --require setup.js --compilers js:babel-register --recursive",
    "test:watch": "npm test -- --watch",
    "build:css": "node-sass --include-path scss styles/main.scss static/css/main.css --importer node_modules/node-sass-import",
    "watch:scss": "nodemon -e scss -x \"npm run build-css\"",
    "dev": "node .",
    "start": "npm-run-all -p dev watch:scss test:watch"
  },
  "keywords": [
    "test",
    "travix"
  ],
  "author": "spartans",
  "license": "ISC",
  "dependencies": {
    "express": "^4.14.0",
    "next": "^4.2.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-form": "^7.2.0",
    "travix-ui-kit": "^0.7.7"
  },
  "devDependencies": {
    "babel-plugin-wrap-in-js": "^1.1.1",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint-config-rallycoding": "^3.2.0",
    "jsdom": "^10.1.0",
    "mocha": "^4.0.1",
    "node-sass": "^4.7.2",
    "node-sass-import": "^1.1.1",
    "nodemon": "^1.12.6",
    "npm-run-all": "^4.1.2",
    "webpack": "^3.10.0"
  }
}
