{
  "name": "test-candidates",
  "version": "0.0.1",
  "description": "test back - front",
  "main": "src/server/index.js",
  "scripts": {
    "test": "yarn run eslint && cross-env NODE_ENV=test nyc ava --verbose",
    "start": "NODE_ENV=hot babel-node src/server/index.js",
    "eslint": "eslint ./",
    "coverage": "nyc report --reporter=lcov",
    "build:server:dev": "webpack --env=dev --config=webpack.server.babel.js --progress --profile --colors",
    "build:server:prod": "webpack --env=prod --config=webpack.server.babel.js --progress --profile --colors",
    "build:browser:dev": "webpack --env=dev --config=webpack.browser.babel.js --progress --profile --colors --json > dist/compilation-stats.json",
    "build:browser:prod": "webpack --env=prod --config=webpack.browser.babel.js --progress --profile --colors --json > dist/compilation-stats.json"
  },
  "keywords": [
    "test",
    "travix"
  ],
  "author": "spartans",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.14.0",
    "immutability-helper": "^2.6.2",
    "lodash": "^4.17.4",
    "normalizr": "^3.2.4",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "travix-logger": "^0.6.4",
    "travix-persistent-object": "^1.0.0"
  },
  "devDependencies": {
    "asset-require-hook": "^1.2.0",
    "autoprefixer": "^7.2.3",
    "ava": "^0.24.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.7",
    "css-modules-require-hook": "^4.2.2",
    "eslint": "^4.14.0",
    "eslint-config-travix": "^4.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "mock-require": "^2.0.2",
    "node-sass": "^4.7.2",
    "nyc": "^11.4.1",
    "postcss-loader": "^2.0.9",
    "react-hot-loader": "^3.1.3",
    "reset.css": "^2.0.2",
    "sass-loader": "^6.0.6",
    "sinon": "^4.1.3",
    "style-loader": "^0.19.1",
    "supertest": "^3.0.0",
    "uglifyjs-webpack-plugin": "^1.1.4",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.3",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-hot-server-middleware": "^0.3.1",
    "webpack-node-externals": "^1.6.0"
  },
  "ava": {
    "files": [
      "src/**/__tests__/**/*.js",
      "src/**/*.spec.js"
    ],
    "require": [
      "babel-register"
    ]
  }
}
