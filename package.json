{
  "name": "test-candidates",
  "version": "0.0.1",
  "description": "test back - front",
  "main": "src/server/index.js",
  "scripts": {
    "test": "yarn run eslint && cross-env NODE_ENV=test nyc ava --verbose",
    "start": "babel-node src/server/index.js",
    "eslint": "eslint ./",
    "coverage": "nyc report --reporter=lcov",
    "build:server:dev": "webpack --env=dev --config=webpack.server.babel.js --progress --profile --colors",
    "build:server:prod": "webpack --env=prod --config=webpack.server.babel.js --progress --profile --colors"
  },
  "keywords": [
    "test",
    "travix"
  ],
  "author": "spartans",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.14.0",
    "normalizr": "^3.2.4",
    "travix-logger": "^0.6.4",
    "travix-persistent-object": "^1.0.0"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.1.3",
    "eslint": "^4.14.0",
    "eslint-config-travix": "^4.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "mock-require": "^2.0.2",
    "nyc": "^11.4.1",
    "sinon": "^4.1.3",
    "supertest": "^3.0.0",
    "uglifyjs-webpack-plugin": "^1.1.4",
    "webpack": "^3.10.0",
    "webpack-node-externals": "^1.6.0"
  },
  "ava": {
    "files": [
      "src/**/__tests__/**/*.js",
      "src/**/*.spec.js"
    ],
    "require": [
      "babel-register"
    ]
  }
}
