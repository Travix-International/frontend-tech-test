{
  "name": "test-candidates",
  "version": "0.0.1",
  "description": "test back - front",
  "main": "server.js",
  "scripts": {
    "start": "node ./server/server.js",
    "dev-server": "webpack-dev-server",
    "dev": "concurrently --kill-others \"npm run start\" \"npm run dev-server\"",
    "test": "REACT_APP_API_HOST='http://localhost:8080' jest --silent",
    "test:snapshots:regenerate": "npm run test -- --updateSnapshot",
    "test:debug": "REACT_APP_API_HOST='http://localhost:8080' jest",
    "test:coverage": "npm run test -- --coverage",
    "build": "webpack"
  },
  "keywords": [
    "test",
    "travix"
  ],
  "author": "spartans",
  "license": "ISC",
  "dependencies": {
    "express": "^4.14.0",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.2",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "4.2.1",
    "reselect": "^3.0.1",
    "superagent": "^3.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.5",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "2.1.2",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^20.0.4",
    "nock": "^9.0.14",
    "node-sass": "^4.5.3",
    "react-addons-test-utils": "^15.6.0",
    "redux-mock-store": "^1.2.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "uuid": "^3.1.0",
    "webpack": "2"
  },
  "jest": {
    "testRegex": "(/__tests__/(.*|(/.|/))).(test|spec).(js?|jsx?)$",
    "collectCoverageFrom": [
      "client/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/dist/**"
    ]
  }
}
