{
  "name": "test-candidates",
  "version": "0.0.1",
  "description": "test back - front",
  "main": "server.js",
  "scripts": {
    "start": "concurrently \"cd client && yarn start\" \"node ./server/server.js\"",
    "install": "concurrently \"cd server && yarn\" \"cd client && yarn\"",
    "build": "cd client && yarn run build",
    "test": "cd client && yarn run test",
    "eject": "cd client && yarn run eject",
    "server": "node ./server/server.js"
  },
  "keywords": [
    "test",
    "travix"
  ],
  "author": "Harm Meijer",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0"
  }
}
