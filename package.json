{
  "name": "test-candidates",
  "version": "0.0.1",
  "description": "test back - front",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "buildDev": "webpack --watch --progress",
    "prod": "rm -rf ./dist | webpack -p && cd ./dist &&  http-server | npm run start",
    "eslint": "eslint client",
    "test": "find ./tests/client/src/utils -name *.test.js | xargs node node_modules/.bin/jest --coverage -u"
  },
  "keywords": [
    "test",
    "travix"
  ],
  "lint-staged": {
    "*.{js,jsx}": "eslint",
    "*.{ts,tsx}": "tslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "globals": {
      "__SERVER__": false
    },
    "verbose": true,
    "setupFiles": [
      "./test-setup.js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "css",
      "scss"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "transform": {
      "^.+\\.jsx$": "babel-jest",
      "^.+\\.js$": "babel-jest"
    },
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.(js?|ts?)$",
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "^client(.*)$": "<rootDir>/client/$1",
      "^utils(.*)$": "<rootDir>/client/src/utils$1",
      "^testutils(.*)$": "<rootDir>/testutils$1"
    }
  },
  "author": "spartans",
  "license": "ISC",
  "dependencies": {
    "babel-eslint": "^8.1.0",
    "css-loader": "^0.28.11",
    "eslint": "6.1.0",
    "express": "^4.14.0",
    "handlebars": "^4.1.2",
    "handlebars-loader": "^1.3.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-redux": "^4.4.9",
    "react-window": "^1.8.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.3.0",
    "sleek-ui": "^3.0.118",
    "style-loader": "^0.13.1",
    "styled-components": "^2.4.0",
    "travix-ui-kit": "^0.16.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.5"
  },
  "devDependencies": {
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "babel-core": "6.26.0",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "clean-webpack-plugin": "0.1.19",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-15.4": "1.2.0",
    "enzyme-to-json": "3.3.1",
    "eslint-config-airbnb": "17.1.1",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "expect": "1.20.2",
    "husky": "^3.0.1",
    "jest": "20.0.4",
    "jest-cli": "20.0.4",
    "jest-fetch-mock": "^2.1.2",
    "jest-resolve": "20.0.4",
    "mini-css-extract-plugin": "^0.8.0",
    "mockdate": "2.0.2",
    "node-sass": "^4.12.0",
    "react-test-renderer": "^15.6.2",
    "redux-mock-store": "1.2.0",
    "sass-loader": "^7.1.0",
    "supertest": "^4.0.2",
    "yaml-loader": "^0.5.0",
    "react-addons-test-utils": "15.4.1",
    "identity-obj-proxy": "3.0.0"
  }
}
