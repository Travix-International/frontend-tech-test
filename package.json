{
  "name": "test-candidates",
  "version": "0.0.1",
  "description": "test back - front",
  "main": "server.js",
  "private": true,
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "server": "node server.js",
    "lint": "stylelint src/**/*.scss && eslint .",
    "build": "webpack",
    "start:dev": "webpack-dev-server --hot"
  },
  "keywords": ["test", "travix"],
  "author": "spartans",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-preset-stage-2": "^6.24.1",
    "body-parser": "^1.18.2",
    "express": "^4.14.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "react-truncate": "^2.3.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "travix-ui-kit": "^0.9.5"
  },
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-travix": "^4.0.0",
    "eslint-plugin-babel": "^5.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "jest": "^22.4.3",
    "node-sass": "^4.8.3",
    "postcss-loader": "^2.1.3",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "stylelint": "^9.2.0",
    "stylelint-config-travix": "^1.2.0",
    "stylelint-scss": "^3.0.0",
    "transfer-webpack-plugin": "^0.1.4",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.1"
  },
  "jest": {
    "verbose": true,
    "coverageDirectory": "coverage/",
    "notify": false,
    "setupFiles": ["<rootDir>/.jest-setup.js"],
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "\\.s?css$": "<rootDir>/__mocks__/styleMock.js"
    },
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "src/*.{js,jsx}",
      "!src/.*",
      "!src/__test__/*",
      "!src/**/__test__/*",
      "!src/**/index.js"
    ]
  }
}
