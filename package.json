{
  "name": "todo-app",
  "version": "0.0.1",
  "description": "Todo app Travix",
  "main": "index.js",
  "scripts": {
    "bundle:vendors": "webpack --config ./vendors/webpack.config.js",
    "start": "npm run dev",
    "env:dev": "cross-env NODE_ENV=development",
    "env:prod": "cross-env NODE_ENV=production",
    "test": "jest",
    "coverage": "npm test -- --coverage",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "lint": "eslint src src-example private webpack.config.js",
    "dev": "npm run bundle:vendors && npm run env:dev -- webpack-dev-server",
    "build:clean": "rimraf \"dist/!(.git*|Procfile)**\"",
    "build:copy": "copyfiles -u 1 public/* public/**/* dist",
    "prebuild": "npm run build:clean && npm run build:copy",
    "build": "npm run env:prod -- webpack"
  },
  "author": "Leandro Oriente <leandroriente@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "frint": "^2.0.1",
    "frint-react": "^2.0.1",
    "frint-store": "^2.0.1",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "rxjs": "^5.4.1",
    "history": "^4.6.1",
    "lodash": "^4.17.4",
    "react-hot-loader": "3.0.0-beta.6",
    "react-modal": "^2.0.6"
  },
  "devDependencies": {
    "@webpack-blocks/dev-server2": "^0.4.0",
    "@webpack-blocks/webpack2": "^0.4.0",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-jest": "^20.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.0",
    "babel-preset-env": "^1.3.3",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-travix": "^1.1.0",
    "babel-register": "^6.9.0",
    "copyfiles": "^1.0.0",
    "cross-env": "^5.0.0",
    "enzyme": "^2.7.0",
    "eslint": "^3.19.0",
    "eslint-config-travix": "^2.3.2",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-no-unused-vars-rest": "^1.0.6",
    "eslint-plugin-react": "^7.1.0",
    "file-loader": "^0.11.0",
    "html-webpack-plugin": "^2.26.0",
    "jest-cli": "^20.0.0",
    "opn-cli": "^3.1.0",
    "raw-loader": "^0.5.1",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.6.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.6.0",
    "webpack-blocks-happypack": "^0.1.3",
    "webpack-blocks-split-vendor": "^0.2.0",
    "webpack-dev-server": "^2.5.0"
  }
}
