{
  "name": "test-candidates",
  "version": "0.0.1",
  "description": "test back - front",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "test": "npm run test --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --dev --prefix client && npm run server && npm run build --prefix client"
  },
  "keywords": [
    "test",
    "travix"
  ],
  "author": "PabloSpaniard",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^3.6.0",
    "cors": "^2.8.4",
    "express": "^4.14.0"
  },
  "devDependencies": {
    "css-loader": "^1.0.0",
    "style-loader": "^0.22.1",
    "nodemon": "^1.17.4"
  }
}