{
  "name": "travix_test",
  "version": "0.0.1",
  "description": "Travix test repository and dependencies",
  "main": "server.js",
  "scripts": {
    "start": "webpack && node server.js",
    "build": "webpack --watch",
    "selenium-setup": "selenium-standalone install",
    "selenium-start": "selenium-standalone start",
    "test-app": "jest --env=jsdom --watchAll",
    "test-api": "mocha \"__tests__/API/**/*.test.js\" --watch",
    "test-e2e": "wdio wdio.conf.js",
    "test-e2e-watch": "wdio wdio.conf.js --watch"
  },
  "author": "Michael Castro",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.2",
    "jest": "^22.1.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "css-loader": "^0.28.9",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "mocha": "^5.0.0",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.0.10",
    "react-test-renderer": "^16.2.0",
    "request": "^2.83.0",
    "sass-loader": "^6.0.6",
    "selenium-standalone": "^6.12.0",
    "style-loader": "^0.19.1",
    "wdio-mocha-framework": "^0.5.12",
    "webdriverio": "^4.10.1",
    "webpack": "^3.10.0"
  },
  "jest": {
    "verbose": true,
    "setupTestFrameworkScriptFile": "<rootDir>config/setupTests.js",
    "collectCoverageFrom": ["src/javascript/**/*.{js,jsx}"],
    "testPathIgnorePatterns": ["<rootDir>[/\\\\](build|docs|node_modules|.tmp|dist)[/\\\\]"],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "roots": ["<rootDir>/src/", "<rootDir>/__tests__/app"],
    "testRegex": "__tests__/app|\\.(test|spec)\\.js?$",
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/fileTransform.js"
    },
    "transformIgnorePatterns": ["[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"]
  }
}
