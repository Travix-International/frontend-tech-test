{
  "presets": [
    [
      "es2015",
      {
        "modules": false
      }
    ],
    "react",
    "stage-0"
  ],
  "plugins": [
    [
      "emotion/babel", {
        "inline": true
      }
    ],
    [
      "transform-runtime", {
        "helpers": false,
        "polyfill": false
      }
    ],
    "transform-decorators-legacy"
  ],
  "env": {
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    }
  }
}
